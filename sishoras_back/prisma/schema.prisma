datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id      Int     @id @default(autoincrement())
  cpf     String  @unique
  senha   String
  tipoDeAcesso  String 
  // Adicione outros campos conforme necess√°rio
}
